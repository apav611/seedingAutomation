{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-web-data"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"FASArchived_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fairfaxadultsoftball.com/c.php?p=1356&Archived=2022#"
		},
		"FallMenThursday1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fairfaxadultsoftball.com/c.php?p=365&Season=F&League=FM&Division=H1&SelectedTeam=All+Teams&Year=2022"
		},
		"FallMenThursday2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fairfaxadultsoftball.com/c.php?p=365&Season=F&League=FM&Division=H2&SelectedTeam=All+Teams&Year=2022"
		},
		"FallMenThursday3_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fairfaxadultsoftball.com/c.php?p=365&Season=F&League=FM&Division=H3&SelectedTeam=All+Teams&Year=2022"
		},
		"FallMenThursday4_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://www.fairfaxadultsoftball.com/c.php?p=365&Season=F&League=FM&Division=H4&SelectedTeam=All+Teams&Year=2022"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://safasseeding.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-web-data_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"FASArchived_properties_typeProperties_url": {
						"value": "[parameters('FASArchived_properties_typeProperties_url')]"
					},
					"FallMenThursday1_properties_typeProperties_url": {
						"value": "[parameters('FallMenThursday1_properties_typeProperties_url')]"
					},
					"FallMenThursday2_properties_typeProperties_url": {
						"value": "[parameters('FallMenThursday2_properties_typeProperties_url')]"
					},
					"FallMenThursday3_properties_typeProperties_url": {
						"value": "[parameters('FallMenThursday3_properties_typeProperties_url')]"
					},
					"FallMenThursday4_properties_typeProperties_url": {
						"value": "[parameters('FallMenThursday4_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}